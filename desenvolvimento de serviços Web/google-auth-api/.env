# Porta onde o servidor da aplicação irá escutar
PORT=3000
# Client ID fornecido pelo Google para autenticação OAuth 2.0
CLIENT_ID=
# Chave secreta usada para assinar e verificar JWTs emitidos pela API
JWT_SECRET=
# Configurações de conexão com o banco de dados MySQL
DB_HOST=localhost # Endereço do servidor MySQL
DB_USER="root" # Nome do usuário do banco MySQL
DB_PASSWORD= # Senha do usuário do banco MySQL
DB_DATABASE=autenticacao_db # Nome do banco de dados utilizado pela aplicação
